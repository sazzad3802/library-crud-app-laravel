                                            }
                                        },
                                        "psuMailGroups": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "fileTypes": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
