        "/common-data": {
            "post": {
                "tags": [
                    "Common Data"
                ],
                "summary": "Fetch common data by type",
                "operationId": "baf8af9235fa9ca2a8be02ce206243c4",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "type"
                                ],
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "example": "PRG"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Records found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/CommonData"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Missing type"
                    },
                    "404": {
                        "description": "No records found"
                    }
                }
            }
        },
        "/common-data/regions": {
            "post": {
                "tags": [
                    "Common Data"
                ],
                "summary": "Fetch regions by type and parent ID",
                "operationId": "1b3ae03a3f5d635dd3fd9f261249a444",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "type",
                                    "parentId"
                                ],
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "example": "RGN"
                                    },
                                    "parentId": {
                                        "type": "integer",
                                        "example": 5
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Records found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/CommonData"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Missing parameters"
                    },
                    "404": {
                        "description": "No records found"
                    }
                }
            }
        },
        "/common-data/{id}": {
            "get": {
                "tags": [
                    "Common Data"
                ],
                "summary": "Fetch a record by ID",
                "operationId": "8ee083146babe26b38d20c444976460b",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Record found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CommonData"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "CommonData": {
                "required": [
                    "id",
                    "type",
                    "name"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "pid": {
                        "type": "integer",
                        "example": null,
                        "nullable": true
                    },
                    "type": {
                        "type": "string",
                        "example": "PRG"
                    },
                    "name": {
                        "type": "string",
                        "example": "Education Program"
                    },
                    "description": {
                        "type": "string",
                        "example": "Program related to education",
                        "nullable": true
                    },
                    "is_active": {
                        "type": "boolean",
                        "example": true
                    },
                    "int_value": {
                        "type": "integer",
                        "example": 5,
                        "nullable": true
                    },
                    "string_value": {
                        "type": "string",
                        "example": "Extra info",
                        "nullable": true
                    },
                    "latitude": {
                        "type": "number",
                        "format": "float",
                        "example": 23.8103,
                        "nullable": true
                    },
                    "longitude": {
                        "type": "number",
                        "format": "float",
                        "example": 90.4125,
                        "nullable": true
                    },
                    "parent_id": {
                        "type": "integer",
                        "example": 10,
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Common Data",
            "description": "Endpoints for managing common data (programs, donors, banks, etc.)"
        }
    ]
}