            "get": {
                "tags": [
                    "Common Data"
                ],
                "summary": "Fetch grouped master data",
                "description": "Returns data grouped by type (programs, donors, banks, etc.)",
                "operationId": "d4324c846c2f03272092f05b32a60ae7",
                "responses": {
                    "200": {
                        "description": "Grouped data by categories",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "programTypes": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "programs": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
