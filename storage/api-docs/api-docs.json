                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "currencies": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "years": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "operationMailGroups": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/CommonData"
                                            }
                                        },
                                        "groups": {
                                            "type": "array",
                                            "items": {
